<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">





    <TextView
        android:id="@+id/nomEU"
        android:layout_width="wrap_content"
        android:textSize="20dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="25dp"
        android:text="TextView" />



    <TextView
        android:id="@+id/mat"
        android:layout_marginLeft="90dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/mat2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:text=""

        android:layout_below="@+id/mat"
        android:layout_alignStart="@+id/mat" />

    <TextView
        android:id="@+id/mat3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/mat2"
        android:textSize="20dp"
        android:text=""
        android:layout_below="@+id/mat2" />

    <TextView
        android:id="@+id/cr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="50dp"
        android:textSize="20dp"
        android:text="TextView"
        android:layout_marginStart="40dp"
        android:layout_alignBaseline="@+id/nomEU"
        android:layout_alignBottom="@+id/nomEU"
        android:layout_toEndOf="@+id/mat2" />

    <TextView
        android:id="@+id/re"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/cr"

        android:layout_marginStart="18dp"
        android:layout_toEndOf="@+id/cr"
        android:textSize="20dp"
        android:text="TextView" />



</RelativeLayout>